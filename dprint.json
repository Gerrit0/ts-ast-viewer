{
  "incremental": true,
  "lineWidth": 160,
  "indentWidth": 2,
  "includes": ["**/*.{ts,tsx,js,jsx,json,md,css}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "public/vs",
    "shared/dist",
    "site/build",
    "site/src/resources",
    "CHANGELOG.md"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.55.0.wasm",
    "https://plugins.dprint.dev/json-0.13.0.wasm",
    "https://plugins.dprint.dev/markdown-0.10.0.wasm",
    "https://plugins.dprint.dev/prettier-0.2.0.exe-plugin@5fc84274198107b5464477803eea335ab3c978738ff40058294b105d2e32d5ae"
  ]
}
